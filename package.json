{
  "name": "slldt-server",
  "version": "0.0.1",
  "description": "server for slldt",
  "main": "app.js",
  "scripts": {
    "start": "npm run build:clean && npm run build && node ./build/index.js",
    "build": "babel ./src --out-dir build",
    "build:clean": "rimraf ./build",
    "lint": "eslint ./src/**/*.js",
    "watch": "babel-watch ./src/index.js",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signalz/slldt-server.git"
  },
  "author": "Son Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signalz/slldt-server/issues"
  },
  "homepage": "https://github.com/signalz/slldt-server#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-auth-token": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.38.0",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-watch": "^2.0.7",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.0.0-rc.15",
    "rimraf": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
